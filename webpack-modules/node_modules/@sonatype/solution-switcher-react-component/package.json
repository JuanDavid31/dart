{
  "name": "@sonatype/solution-switcher-react-component",
  "version": "12.12.0",
  "description": "Solution Switcher React Component",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "npm-run-all -p lint compile-ts copy-assets copy-scss copy-package-json",
    "watch": "npm-run-all -p lint compile-ts-watch copy-assets-watch copy-scss-watch copy-package-json-watch",
    "compile-ts-watch": "tsc -w --project tsconfig.build.json",
    "compile-ts": "tsc --project tsconfig.build.json",
    "copy-assets": "cpx \"src/components/SolutionSwitcher/images/**/*\" dist/components/SolutionSwitcher/images",
    "copy-assets-watch": "cpx -w \"src/components/SolutionSwitcher/images/**/*\" dist/components/SolutionSwitcher/images",
    "copy-scss": "cpx \"src/components/SolutionSwitcher/**/*.scss\" dist/components/SolutionSwitcher/",
    "copy-scss-watch": "cpx -w \"src/components/SolutionSwitcher/**/*.scss\" dist/components/SolutionSwitcher/",
    "copy-package-json": "cpx package.json dist",
    "copy-package-json-watch": "cpx -w package.json dist",
    "lint": "eslint \"./src/**/*.{js,ts}{,x}\"",
    "lint:build": "eslint -f checkstyle \"./src/**/*.{js,ts}{,x}\" > eslint.xml",
    "lint:fix": "eslint --fix \"./src/**/*.{js,ts}{,x}\"",
    "clean": "rimraf dist webpack-modules",
    "test": "jest",
    "test-watch": "jest --watch",
    "test:ci": "jest --maxWorkers=50% --ci --no-cache || true",
    "webpack": "webpack --env production --mode=production",
    "webpack-dev-server": "webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype/solution-switcher"
  },
  "author": "Sonatype, Inc.",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@sonatype/react-shared-components": "^12.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "axios": "^1.6.8",
    "axios-mock-adapter": "^1.22.0",
    "chromedriver": "^116.0.0",
    "dompurify": "^3.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "resolve-url-loader": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.17.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-loader": "^8.2.2",
    "copy-modules-webpack-plugin": "^2.2.1",
    "copy-webpack-plugin": "^9.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.53.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.4",
    "typescript": "4.7.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.13.1"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "resolutions": {
    "strip-ansi": "6.0.1",
    "unset-value": "2.0.1"
  }
}
